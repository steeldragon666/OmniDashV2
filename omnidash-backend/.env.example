# Database
DATABASE_URL="postgresql://username:password@localhost:5432/omnidash_db?schema=public"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Session
SESSION_SECRET="your-session-secret-key-change-in-production"

# Redis
REDIS_URL="redis://localhost:6379"

# Server
PORT=3000
NODE_ENV="development"
API_URL="http://localhost:3000"
FRONTEND_URL="http://localhost:3001"

# OAuth Providers
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
GOOGLE_REDIRECT_URI="http://localhost:3000/auth/google/callback"

GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"

MICROSOFT_CLIENT_ID="your-microsoft-client-id"
MICROSOFT_CLIENT_SECRET="your-microsoft-client-secret"

# AI Providers
OPENAI_API_KEY="your-openai-api-key"
CLAUDE_API_KEY="your-anthropic-claude-api-key"

# Social Media APIs
TWITTER_API_KEY="your-twitter-api-key"
TWITTER_API_SECRET="your-twitter-api-secret"
TWITTER_BEARER_TOKEN="your-twitter-bearer-token"
TWITTER_CLIENT_ID="your-twitter-client-id-v2"
TWITTER_CLIENT_SECRET="your-twitter-client-secret-v2"

INSTAGRAM_APP_ID="your-instagram-app-id"
INSTAGRAM_APP_SECRET="your-instagram-app-secret"

LINKEDIN_CLIENT_ID="your-linkedin-client-id"
LINKEDIN_CLIENT_SECRET="your-linkedin-client-secret"

TIKTOK_CLIENT_ID="your-tiktok-client-id"
TIKTOK_CLIENT_SECRET="your-tiktok-client-secret"
TIKTOK_CLIENT_KEY="your-tiktok-client-key"

FACEBOOK_APP_ID="your-facebook-app-id"
FACEBOOK_APP_SECRET="your-facebook-app-secret"

# Google Services
GOOGLE_ANALYTICS_VIEW_ID="your-ga-view-id"
GOOGLE_ANALYTICS_CREDENTIALS="your-google-analytics-service-account-json"
GOOGLE_DRIVE_FOLDER_ID="your-drive-folder-id"

# N8N Integration
N8N_API_URL="http://localhost:5678/api/v1"
N8N_API_KEY="your-n8n-api-key"

# Supabase
SUPABASE_URL="your-supabase-project-url"
SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"
SUPABASE_BUCKET="omnidash-files"

# File Storage (Alternative to Supabase)
AWS_REGION="us-east-1"
AWS_BUCKET="omnidash-media"
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"

# Email Service
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-email-password"

# Monitoring & Logging
LOG_LEVEL="info"
SENTRY_DSN="your-sentry-dsn"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=1000

# Encryption
ENCRYPTION_KEY="your-32-character-encryption-key-here"