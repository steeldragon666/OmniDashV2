{
  "name": "omnidash-frontend",
  "version": "1.0.0",
  "description": "World-class frontend for OmniDash",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev -p 3001",
    "build": "node build-prod.js",
    "build:next": "next build",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start -p 3001",
    "lint": "next lint",
    "migrate": "node -e \"console.log('Run migrations: npx supabase db push')\"",
    "security:audit": "npm audit && npm audit fix",
    "security:check": "node -e \"require('./lib/env').validateEnvironment(); console.log('âœ… Environment validation passed')\"",
    "security:test": "npm run security:audit && npm run security:check",
    "logs:view": "tail -f logs/application-$(date +%Y-%m-%d).log",
    "logs:errors": "tail -f logs/error-$(date +%Y-%m-%d).log",
    "test:security": "echo 'ðŸ”’ Running security tests...' && npm run security:test && echo 'âœ… Security tests passed'",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:ci && npm run test:e2e",
    "db:setup": "node setup-database.js",
    "db:deploy": "node deploy-supabase.js",
    "db:reset": "node deploy-supabase.js --reset",
    "test:connections": "node test-connections.js",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "production:ready": "npm run build && node production-check.js",
    "production:check": "node production-check.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@playwright/test": "^1.55.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.57.0",
    "@types/nodemailer": "^7.0.1",
    "bullmq": "^5.58.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "dotenv": "^17.2.2",
    "express-rate-limit": "^8.1.0",
    "framer-motion": "^10.16.4",
    "googleapis": "^159.0.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^5.18.1",
    "rate-limiter-flexible": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.0.0",
    "twitter-api-v2": "^1.25.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
